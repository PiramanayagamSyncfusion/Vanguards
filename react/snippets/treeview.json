            "\t}",
            "];",
            "\tpublic fields: Object = { dataSource: this.${6:dataSource}, id: '${2:id}', text: '${3:text}', child: '${4:child}', selected: '${5:isSelected}' };",
            "\tpublic allowDragAndDrop : boolean = true;",
            "\trender() {",
