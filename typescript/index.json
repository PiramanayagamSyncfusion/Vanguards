		"body": [
			"import { ComboBox } from '@syncfusion/ej2-dropdowns';",
			"import { Query, DataManager, ODataV4Adaptor } from '@syncfusion/ej2-data';",
			"",
			"let dataSource: DataManager = new DataManager({",
