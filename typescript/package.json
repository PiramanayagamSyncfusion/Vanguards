{
    "name": "ej2-packages",
    "displayName": "TypeScript snippet for Essential JS 2 components",
    "description": "TypeScript snippet for Essential JS 2 components",
    "version": "1.13.0",
    "publisher": "gurunathan",
    "engines": {
        "vscode": "^1.8.0"
    },
    "icon": "images/synclogo.png",
    "galleryBanner": {
        "color": "#0273D4",
        "theme": "dark"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "typescript",
                "path": "./snippets/index.json"
            }
        ],
        "activationEvents": [
            "onLanguage:typescript"
        ]
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/PiramanayagamSyncfusion/Vanguards.git"
    },
    "devDependencies": {
        "gulp": "latest",
        "gulp-merge-json": "latest"
    },
    "scripts": {
        "vscode:prepublish": "gulp merge"
    },
    "keywords": [
        "TypeScript",
        "Syncfusion",
        "TreeView",
        "ej2",
        "ej2-treeview",
        "ej2-navigation"
    ]
}